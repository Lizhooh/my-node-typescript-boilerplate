{
    "name": "myapp",
    "version": "0.0.0",
    "description": "",
    "engines": {
        "node": ">= 9"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development concurrently \"npm run build:watch\" \"npm run watch\"",
        "server": "cross-env NODE_ENV=production node build/src/index.js",
        "pm2": "cross-env NODE_ENV=production pm2 start build/src/index.js -i 4",
        "watch": "nodemon build/src/index.js",
        "clean": "rimraf coverage build tmp",
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc -w -p tsconfig.json",
        "test": "jest --coverage",
        "testv": "majestic --port 3008 --noOpen"
    },
    "dependencies": {
        "colors": "^1.3.3",
        "config": "^3.1.0",
        "dayjs": "^1.8.14",
        "got": "^9.6.0",
