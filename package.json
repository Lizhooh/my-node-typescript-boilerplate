        "koa-logger": "^3.2.1",
        "koa-router": "^7.4.0",
        "lodash": "^4.17.15",
        "qs": "^6.7.0",
        "tslib": "~1.9.3"
    },
    "devDependencies": {
        "@types/qs": "^6.5.3",
        "@types/config": "^0.0.34",
        "@types/debug": "^4.1.4",
        "@types/got": "^9.6.2",
        "@types/jest": "^24.0.0",
        "@types/koa": "^2.0.49",
        "@types/koa-router": "^7.0.42",
        "@types/lodash": "^4.14.136",
        "@types/node": "^10.14.0",
        "babel-eslint": "^8.2.3",
        "concurrently": "^4.1.1",
