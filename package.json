        "eslint-plugin-typescript": "^0.14.0",
        "jest": "~24.8.0",
        "majestic": "^1.6.0",
        "nodemon": "^1.19.1",
        "prettier": "~1.17.0",
        "rimraf": "^2.6.3",
        "ts-jest": "^24.0.0",
        "tslint": "~5.16.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-microsoft-contrib": "~6.1.0",
        "tsutils": "~3.10.0",
        "typescript": "~3.4.0"
